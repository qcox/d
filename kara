						if Target.BodyEffects:FindFirstChild("K.O") == nil then
							HasDied = true

						end
						if HasDied == false then
							bodyp.Position = Target.PrimaryPart.Position
						end
					end)
				until HasDied == true

				Party:Destroy()
				for i = 1, 10 do
					game.Players.LocalPlayer.Character:MoveTo(Target.LowerTorso.Position)
					wait(0.1)
				end
				break
			end

		end


		CylinderMesh2.Parent = Part1
		CylinderMesh2.Scale = Vector3.new(0.100000001, 0.100000001, 0.100000001)
		CylinderMesh2.Scale = Vector3.new(0.100000001, 0.100000001, 0.100000001)
